{
  "name": "@exotjs/express",
  "version": "0.1.0",
  "description": "An Express middleware for Exot Inspector.",
  "author": "Daniel Regeci",
  "license": "MIT",
  "keywords": [
    "exot",
    "inspector",
    "express",
    "middleware"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "eslint": "eslint ./lib/**/*",
    "format": "prettier --write './(lib|tests)/**/*'",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./inspector": {
      "types": "./dist/inspector.d.ts",
      "import": "./dist/inspector.js"
    },
    "./store": {
      "types": "./dist/store.d.ts",
      "import": "./dist/store.js"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types"
      ]
    },
    "./inspector": {
      "types": [
        "./dist/inspector"
      ]
    },
    "./store": {
      "types": [
        "./dist/store"
      ]
    }
  },
  "peerDependencies": {
    "@exotjs/inspector": "0.x",
    "ws": "8.x"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.1"
  }
}
